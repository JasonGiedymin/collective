{
  "name": "role-dev-node",
  "chef_type": "role",
  "json_class": "Chef::Role",
  "default_attributes": {
  },
  "description": "Base Dev Node Role",
  "run_list": [
    "recipe[apt]",
    "recipe[rvm::system]",
    "recipe[rvm::vagrant]",
    "recipe[golang]",
    "recipe[nodejs::install_from_binary]",
    "recipe[nodejs::npm]",
    "recipe[docker]",
    "recipe[chef-client]",
    "recipe[java::oracle]",
    "recipe[scala]",
    "recipe[mesos::build_from_source]"
  ],
  "env_run_lists" : {},
  "override_attributes": {
    "chef_server_url": "https://10.10.10.10",
    "node_name": "dev-node",

    "rvm": {
      "rubies": [
      ],
      "global_gems": [
      ]
    },

    "java": {
      "install_flavor": "oracle",
      "oracle": {
        "accept_oracle_download_terms": true
      },
      "jdk_version": "7"
    },

    "nodejs": {
    },

    "scala": {
      "version": "2.10.3",
      "url": "http://www.scala-lang.org/files/archive/scala-2.10.3.tgz"
    }

  }
}