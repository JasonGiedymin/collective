{
  "name": "role-test",
  "chef_type": "role",
  "json_class": "Chef::Role",
  "default_attributes": {
  },
  "description": "Test Role",
  "run_list": [
    "recipe[apt::default]",
    "recipe[docker]",
    "recipe[java::oracle]",
    "recipe[scala]",
    "recipe[mesos::build_from_source]"
  ],
  "env_run_lists" : {},
  "override_attributes": {
    
    "java": {
      "install_flavor": "oracle",
      "oracle": {
        "accept_oracle_download_terms": true
      },
      "jdk_version": "7"
    },
    "scala": {
      "version": "2.10.3",
      "url": "http://www.scala-lang.org/files/archive/scala-2.10.3.tgz"
    }

  }
}